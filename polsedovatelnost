/*Задана последовательность неотрицательных целых чисел. 
Требуется определить длину последовательности, 
завершающуюся первым встретившимся нолем (число 0 не входит в последовательность).*/
#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int n,mx=0;
     
    while (true)
    { 
        cin>>n;
        if(n==0)
        {
            break;
        }
        mx++;
         
 
    } 
    cout << mx;
     
}   